{
  "id": "cili",
  "name": "无极磁链【慢】",
  "proxy": false,
  "publish": "https://cili.st",
  "url": "https://6mag.net",
  "referer": "https://6mag.net",
  "path": {
    "default": "/search?q={k}"
  },
  "parse": {
    "item": {
      "type": "url",
      "xpath": "//table[contains(@class,'file-list')]//td/a/@href",
      "startIndex": 0
    },
    "page": [
      {
        "type": null,
        "default": 0
      }
    ],
    "magnet": [
      {
        "type": "xpath",
        "xpath": "//input[@id='input-magnet']/@value"
      }
    ],
    "name": [
      {
        "type": "xpath",
        "xpath": "//h2[@class='magnet-title']/text()"
      }
    ],
    "hot": [
      {
        "type": null,
        "default": 99
      }
    ],
    "size": [
      {
        "type": "xpath",
        "xpath": "//dl[contains(@class,'torrent-info')]/dd[last()]/text()"
      }
    ],
    "time": [
      {
        "type": null,
        "default": "2000-00-00"
      }
    ]
  }
}