{
  "id": "cursor",
  "name": "光标网",
  "desc": "",
  "proxy": false,
  "publish": "https://cursor.vip/vip",
  "publishMail": "",
  "url": "https://www.sokankan2.cc",
  "referer": "https://www.sokankan2.cc",
  "path": {
    "default": "/search/{k}/page-{p}.html"
  },
  "parse": {
    "item": {
      "type": "xpath",
      "xpath": "//div[@class='list-view']/article[@class='item' and @data-key]",
      "startIndex": 0
    },
    "page": [
      {
        "type": null,
        "default": 0
      }
    ],
    "magnet": [
      {
        "type": "xpath",
        "xpath": "//a/@href"
      },
      {
        "type": "subIndex",
        "start": -45,
        "end": -5
      }
    ],
    "name": [
      {
        "type": "xpath",
        "xpath": "//h4/text()"
      }
    ],
    "hot": [
      {
        "type": "xpath",
        "xpath": "//p[1]/text()"
      },
      {
        "type": "regex",
        "regex": "Hot：([\\d.]+)"
      }
    ],
    "size": [
      {
        "type": "xpath",
        "xpath": "//p[1]/text()"
      },
      {
        "type": "regex",
        "regex": "Size：([\\d.]+\\s[A-Z].)"
      }
    ],
    "time": [
      {
        "type": "xpath",
        "xpath": "//p[1]/text()"
      },
      {
        "type": "regex",
        "regex": "Created：(\\d{4}-\\d{2}-\\d{2}\\s\\d{2}:\\d{2}:\\d{2})"
      }
    ]
  }
}