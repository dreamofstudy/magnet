{
  "id": "btgg",
  "name": "BTGG",
  "proxy": false,
  "publish": "",
  "publishMail": "",
  "url": "https://www.btgg.in",
  "referer": "https://www.btgg.in",
  "params": {
    "default": "/search?q={k}&p={p}"
  },
  "parse": {
    "item": {
      "xpath": "//div[@class='list']/div[@class='item']",
      "startIndex": 0
    },
    "magnet": [
      {
        "type": "xpath",
        "xpath": "//div[@class='meta']/a/@href"
      }
    ],
    "name": [
      {
        "type": "xpath",
        "xpath": "//div[@class='name']/a/text()"
      }
    ],
    "hot": [
      {
        "type": null,
        "default": 99
      }
    ],
    "size": [
      {
        "type": "xpath",
        "xpath": "//div[@class='meta']/span[1]/text()"
      },
      {
        "type": "subIndex",
        "start": 0,
        "end": -2
      }
    ],
    "time": [
      {
        "type": "xpath",
        "xpath": "//div[@class='meta']/span[3]/text()"
      },
      {
        "type": "subIndex",
        "start": 0,
        "end": -2
      }
    ]
  }
}