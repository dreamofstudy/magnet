{
  "id": "btsow_shadow",
  "name": "BTSOW(非代理)",
  "proxy": false,
  "url": "https://btsow.one",
  "params": {
    "default": "/search/{k}/page/{p}"
  },
  "parse": {
    "item": {
      "xpath": "//div[@class='data-list']/div[@class='row']"
    },
    "magnet": [
      {
        "type": "xpath",
        "xpath": "//a/@href"
      },
      {
        "type": "subIndex",
        "start": -40,
        "end": null
      }
    ],
    "name": [
      {
        "type": "xpath",
        "xpath": "//a/@title"
      }
    ],
    "hot": [
      {
        "type": null,
        "default": 99
      }
    ],
    "size": [
      {
        "type": "xpath",
        "xpath": "//div[contains(@class,'text-right size')]/text()"
      }
    ],
    "time": [
      {
        "type": "xpath",
        "xpath": "//div[contains(@class,'text-right date')]/text()"
      }
    ]
  }
}